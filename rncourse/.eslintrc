{
   "parser": "babel-eslint",
   "plugins": ["react", "react-native"],
   "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb"],
   "rules": {
      "indent": [
         "error",
         3,
         {
            "SwitchCase": 1
         }
      ],
      "react/jsx-indent": ["error", 3],
      "react/jsx-indent-props": ["error", 3],
      "brace-style": ["error", "stroustrup"],
      "react/jsx-filename-extension": "off"
   }
}
